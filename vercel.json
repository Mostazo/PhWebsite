{
  "version": 3,
  "routes": [
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/prompts/(.*)", "dest": "/prompts/$1" }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    },
    {
      "source": "/prompts/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ]
}
